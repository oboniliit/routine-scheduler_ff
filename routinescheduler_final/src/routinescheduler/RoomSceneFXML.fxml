<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: DarkGreen;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="routinescheduler.RoomSceneFXMLController">
   <children>
      <TableView fx:id="roomlistview" onMousePressed="#onRowSelect" prefHeight="400.0" prefWidth="427.0">
        <columns>
          <TableColumn fx:id="no" prefWidth="86.0" text="Room No" />
          <TableColumn fx:id="floor" prefWidth="93.0" text="Floor" />
            <TableColumn fx:id="type" prefWidth="155.0" text="Room Type" />
            <TableColumn fx:id="capacity" prefWidth="91.0" text="Capacity" />
        </columns>
      </TableView>
      <Button layoutX="460.0" layoutY="32.0" mnemonicParsing="false" onAction="#onAddRoom" prefHeight="43.0" prefWidth="126.0" style="-fx-background-color: LightGreen;" text="Add Room">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button layoutX="460.0" layoutY="102.0" mnemonicParsing="false" onAction="#onRemoveRoom" prefHeight="43.0" prefWidth="126.0" style="-fx-background-color: LightGreen;" text="Remove Room">
         <font>
            <Font size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
