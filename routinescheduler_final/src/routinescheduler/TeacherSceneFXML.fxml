<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="403.0" prefWidth="670.0" style="-fx-background-color: Maroon;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="routinescheduler.TeacherSceneFXMLController">
   <children>
      <TableView fx:id="teachertableview" onMousePressed="#onRowSelect" prefHeight="403.0" prefWidth="516.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="67.0" text="Teacher Id" />
          <TableColumn fx:id="name" prefWidth="143.0" text="Teacher Name" />
            <TableColumn fx:id="designation" minWidth="7.0" prefWidth="99.0" text="Designation" />
            <TableColumn fx:id="department" prefWidth="78.0" text="Department" />
            <TableColumn fx:id="email" prefWidth="127.0" text="Email Id" />
        </columns>
      </TableView>
      <Button layoutX="527.0" layoutY="25.0" mnemonicParsing="false" onAction="#onAddTeacher" prefHeight="39.0" prefWidth="134.0" style="-fx-background-color: BurlyWood;" text="Add Teacher">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Button layoutX="527.0" layoutY="95.0" mnemonicParsing="false" onAction="#onRemoveTeacher" prefHeight="39.0" prefWidth="134.0" style="-fx-background-color: BurlyWood;" text="Remove Teacher">
         <font>
            <Font size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
